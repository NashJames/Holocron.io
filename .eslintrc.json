{
  "extends": ["next/core-web-vitals", "eslint:recommended", "prettier"],
  "parser": "@typescript-eslint/parser",
  "ignorePatterns": [".next"],
  "root": true,
  "parserOptions": {
    "ecmaVersion": 13
  },
  "rules": {
    "semi": ["warn", "never"],
    "linebreak-style": ["error", "unix"],
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }]
  }
}
