  "homepage": "https://github.com/NashJames/Holocron.io/",
  "repository": "github:NashJames/Holocron.io",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "tsc && next build",
    "format": "next lint --fix && pnpm lint:styles --fix && prettier --write .",
    "format:diff": "next lint && pnpm lint:styles && prettier --check .",
    "lint:styles": "stylelint '**/*.{css,scss}' --report-needless-disables --report-invalid-scope-disables --allow-empty-input",
    "clean": "rm -rf node_modules .next tsconfig.tsbuildinfo next-env.d.ts",
    "test": "pnpm build && pnpm dlx playwright test",
    "test:codegen": "pnpm dlx playwright codegen"
  },
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/view": "^6.7.3",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@lezer/highlight": "^1.1.3",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.6",
    "@next/font": "^13.1.5",
    "@uiw/codemirror-themes": "^4.19.7",
    "@uiw/react-codemirror": "^4.19.7",
    "next": "^13.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.0.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.29.2",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "eslint": "^8.32.0",
    "eslint-config-next": "^13.1.5",
    "eslint-config-prettier": "^8.6.0",
    "prettier": "^2.8.3",
    "sass": "^1.57.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": "^16",
    "pnpm": "^7"
  },
  "packageManager": "pnpm@7.16.0",
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2
  }

}
